        <typeAlias alias="DataDictionary" type="com.grizzly.model.DataDictionary"/>
        <typeAlias alias="DevUser" type="com.grizzly.model.DevUser"/>
    </typeAliases>
    <mappers>
        <mapper resource="com/grizzly/dao/NewsMapper.xml"/>
        <mapper resource="com/grizzly/dao/DeveloperMapper.xml"/>
        <mapper resource="com/grizzly/dao/AppCategoryMapper.xml"/>
        <mapper resource="com/grizzly/dao/AppInfoMapper.xml"/>
        <mapper resource="com/grizzly/dao/AppVersionMapper.xml"/>
