<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
        <typeAlias alias="News" type="com.grizzly.model.News"/>
        <typeAlias alias="AdPromotion" type="com.grizzly.model.AdPromotion"/>
        <typeAlias alias="AppCategory" type="com.grizzly.model.AppCategory"/>
        <typeAlias alias="AppInfo" type="com.grizzly.model.AppInfo"/>
        <typeAlias alias="AppVersion" type="com.grizzly.model.AppVersion"/>
